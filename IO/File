import java.io.File;

public class Test1 {
    public static void main(String[] args) {
        File f = new File("d:" + File.separator+"VNC"+ File.separator+"VNC");
        print(f);
    }
    public static void print(File f) {
        if (f != null) 
		{
            if (f.isDirectory()) 
			{
                File[] files = f.listFiles();
                if (files != null) 
				{
                    for (File file : files) 
					{
                            print(file);
                    }
                }
            } else 
			{
                System.out.println(f);
            }
        }
    }
}
